	</packageSources>

	<packageSourceCredentials>
		<Gecko_x0020_MyGet_x0020_private_x0020_package_x0020_source>
			<add key="Username" value="ephorteread" />
			<add key="ClearTextPassword" value="ReadingMyGetPackages123" />
		</Gecko_x0020_MyGet_x0020_private_x0020_package_x0020_source>
